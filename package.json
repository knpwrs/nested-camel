{
  "name": "nested-camel",
  "version": "1.1.0",
  "description": "Flattens a nested object tree by recursively joining and camelcasing keys.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -sd lib src",
    "test": "nyc ava src/**/*.test.js",
    "test:watch": "ava --watch src/**/*.test.js",
    "lint": "eslint src",
    "posttest": "npm run lint",
    "prepack": "npm run build"
  },
  "ava": {
    "require": "@babel/register"
  },
  "author": "Kenneth Powers <ken@kenpowers.net> (http://knpw.rs)",
  "repository": {
    "type": "git",
    "url": "https://github.com/knpwrs/nested-camel"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "glamor": "^2.20.40",
    "nyc": "^13.1.0"
  }
}
